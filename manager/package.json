    "prettier": "3.4.2",
    "prettier-plugin-svelte": "3.3.3",
    "rdf-string": "2.0.1",
    "redis": "^4.7.0",
    "robots-parser": "3.0.1",
    "sigma": "3.0.2",
    "streamify-string": "^1.0.1",
    "svelte": "5.19.5",
    "svelte-check": "4.1.4",
    "svelte-icons-pack": "3.1.3",
    "swagger-ui": "5.19.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "vite": "6.0.11",
    "vitest": "3.0.4",
    "winston": "^3.17.0",
    "yazl": "3.3.1"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.42",
    "@types/content-type": "^1.1.8",
    "@types/lodash": "^4.17.15",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.12.0",
    "@types/streamify-string": "^1.0.4",
    "@types/swagger-ui": "3.52.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
