{
  "name": "derzis-manager",
  "version": "1.1.0",
  "description": "Derzis: a Linked Data crawler",
  "author": "Andr√© Santos <andrefs@andrefs.com>",
  "license": "MIT",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "NODE_OPTS='--max-old-space-size=55296' vite dev",
    "serve": "NODE_OPTS='--max-old-space-size=55296' vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "typecheck": "tsc -b tsconfig.json",
    "db:drop:dev": "npx tsx src/scripts/drop-dev-db.ts",
    "typecheck:watch": "tsc -b tsconfig.json --watch"
  },
  "dependencies": {
    "@playwright/test": "1.50.0",
    "@sendgrid/mail": "8.1.4",
    "@sigma/export-image": "3.0.0",
    "@sveltejs/adapter-auto": "4.0.0",
    "@sveltejs/adapter-node": "5.2.12",
    "@sveltejs/kit": "2.16.1",
    "@sveltestrap/sveltestrap": "7.0.3",
    "@typegoose/typegoose": "12.10.1",
    "@types/mongodb-uri": "0.9.4",
    "@types/n3": "1.21.1",
    "@types/nodemailer": "6.4.17",
    "@types/yazl": "3.3.0",
    "axios": "^1.7.9",
    "bluebird": "^3.7.2",
    "bootstrap": "5.3.3",
    "chalk": "^5.4.1",
    "cheerio": "^1.0.0",
    "content-type": "^1.0.5",
    "dotenv": "^16.4.7",
    "express": "4.21.2",
    "express-handlebars": "^8.0.1",
    "graphology": "0.26.0",
    "graphology-layout-forceatlas2": "0.10.1",
    "http-link-header": "^1.1.3",
    "humanize-digest": "^1.0.0",
    "lodash": "^4.17.21",
    "mailtrap": "3.4.0",
    "mongodb-uri": "0.9.7",
    "morgan": "^1.10.0",
    "n3": "1.23.1",
